create or replace FUNCTION FN_FECHA_VEN_EXP 
(
  FECHA IN VARCHAR2 
, TIPO_FECHA IN VARCHAR2 
) RETURN VARCHAR2 AS
respuesta VARCHAR2(20) := NULL;
BEGIN
  IF TIPO_FECHA = 'VEN' THEN
    respuesta := TO_CHAR(FECHA);
  ELSIF TIPO_FECHA = 'EXP' THEN
    respuesta := TO_CHAR(FECHA);
  END IF;
  RETURN respuesta;
END FN_FECHA_VEN_EXP;